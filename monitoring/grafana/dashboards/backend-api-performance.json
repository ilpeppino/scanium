{
  "uid": "scanium-backend-api-perf",
  "title": "Scanium - Backend API Performance",
  "description": "Detailed API performance analysis: latency, throughput, and errors by route",
  "tags": ["scanium", "backend", "api", "performance", "latency"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "label": "Service",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "MIMIR" },
        "query": "label_values(scanium_http_request_duration_ms_bucket, service_name)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "text": "All", "value": "$__all" }
      },
      {
        "name": "env",
        "label": "Environment",
        "type": "custom",
        "query": "dev,stage,prod",
        "current": { "text": "All", "value": "$__all" },
        "options": [
          { "text": "All", "value": "$__all", "selected": true },
          { "text": "dev", "value": "dev", "selected": false },
          { "text": "stage", "value": "stage", "selected": false },
          { "text": "prod", "value": "prod", "selected": false }
        ],
        "includeAll": true,
        "multi": true
      },
      {
        "name": "route",
        "label": "Route",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "MIMIR" },
        "query": "label_values(scanium_http_request_duration_ms_bucket{service_name=~\"$service\"}, route)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "text": "All", "value": "$__all" }
      },
      {
        "name": "method",
        "label": "HTTP Method",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "MIMIR" },
        "query": "label_values(scanium_http_request_duration_ms_bucket, method)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Latency Overview",
      "type": "row",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 2,
      "title": "p50 Latency",
      "description": "Median request latency",
      "type": "stat",
      "gridPos": { "x": 0, "y": 1, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.50, sum by (le) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m])))",
          "legendFormat": "p50"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 300 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "id": 3,
      "title": "p95 Latency",
      "description": "95th percentile latency",
      "type": "stat",
      "gridPos": { "x": 6, "y": 1, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m])))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 200 },
              { "color": "red", "value": 500 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "id": 4,
      "title": "p99 Latency",
      "description": "99th percentile latency (tail latency)",
      "type": "stat",
      "gridPos": { "x": 12, "y": 1, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (le) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m])))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "id": 5,
      "title": "Throughput (req/s)",
      "description": "Requests per second",
      "type": "stat",
      "gridPos": { "x": 18, "y": 1, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(scanium_http_requests_total{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m]))",
          "legendFormat": "req/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "id": 6,
      "title": "Latency Percentiles Over Time",
      "description": "p50, p90, p95, p99 latency trends",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.50, sum by (le) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m])))",
          "legendFormat": "p50"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.90, sum by (le) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m])))",
          "legendFormat": "p90"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m])))",
          "legendFormat": "p95"
        },
        {
          "refId": "D",
          "expr": "histogram_quantile(0.99, sum by (le) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m])))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "spanNulls": true
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max", "last"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 7,
      "title": "Route Analysis",
      "type": "row",
      "gridPos": { "x": 0, "y": 13, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 8,
      "title": "Latency by Route (p95)",
      "description": "95th percentile latency per route",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 14, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, histogram_quantile(0.95, sum by (le, route) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m]))))",
          "legendFormat": "{{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 9,
      "title": "Throughput by Route",
      "description": "Request rate per route",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 14, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (route) (rate(scanium_http_requests_total{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m])))",
          "legendFormat": "{{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "stacking": { "mode": "normal" }
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "sum"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 10,
      "title": "Slowest Routes (p95 > 200ms)",
      "description": "Routes with high latency requiring attention",
      "type": "table",
      "gridPos": { "x": 0, "y": 22, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(15, histogram_quantile(0.95, sum by (le, route, service_name) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\"}[15m])))) > 200",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "le": true },
            "renameByName": {
              "route": "Route",
              "service_name": "Service",
              "Value": "p95 (ms)"
            }
          }
        },
        {
          "id": "sortBy",
          "options": { "sort": [{ "field": "p95 (ms)", "desc": true }] }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "p95 (ms)" },
            "properties": [
              { "id": "unit", "value": "ms" },
              { "id": "decimals", "value": 1 },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "yellow", "value": null },
                    { "color": "orange", "value": 500 },
                    { "color": "red", "value": 1000 }
                  ]
                }
              },
              { "id": "custom.cellOptions", "value": { "type": "color-background" } }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "p95 (ms)", "desc": true }]
      }
    },
    {
      "id": 11,
      "title": "Route Error Rate",
      "description": "Error rate percentage by route",
      "type": "table",
      "gridPos": { "x": 12, "y": 22, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(15, sum by (route, service_name) (rate(scanium_http_requests_total{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", status_code=~\"5..\"}[15m])) / sum by (route, service_name) (rate(scanium_http_requests_total{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\"}[15m])) * 100)",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": {
              "route": "Route",
              "service_name": "Service",
              "Value": "Error Rate (%)"
            }
          }
        },
        {
          "id": "sortBy",
          "options": { "sort": [{ "field": "Error Rate (%)", "desc": true }] }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Error Rate (%)" },
            "properties": [
              { "id": "unit", "value": "percent" },
              { "id": "decimals", "value": 2 },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "green", "value": null },
                    { "color": "yellow", "value": 1 },
                    { "color": "red", "value": 5 }
                  ]
                }
              },
              { "id": "custom.cellOptions", "value": { "type": "color-background" } }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true
      }
    },
    {
      "id": 12,
      "title": "HTTP Method Analysis",
      "type": "row",
      "gridPos": { "x": 0, "y": 30, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 13,
      "title": "Latency by HTTP Method (p95)",
      "description": "Latency breakdown by GET, POST, PUT, DELETE, etc.",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 31, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, method) (rate(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m])))",
          "legendFormat": "{{method}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 14,
      "title": "Throughput by HTTP Method",
      "description": "Request rate by HTTP method",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 31, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (method) (rate(scanium_http_requests_total{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[5m]))",
          "legendFormat": "{{method}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "lineWidth": 1,
            "stacking": { "mode": "normal" }
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "sum"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 15,
      "title": "Latency Heatmap",
      "description": "Latency distribution heatmap showing request density",
      "type": "heatmap",
      "gridPos": { "x": 0, "y": 39, "w": 24, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(scanium_http_request_duration_ms_bucket{service_name=~\"$service\", deployment_environment=~\"$env\", route=~\"$route\", method=~\"$method\"}[$__rate_interval])) by (le)",
          "format": "heatmap",
          "legendFormat": "{{le}}"
        }
      ],
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": {
          "scheme": "Spectral",
          "mode": "scheme",
          "reverse": true
        },
        "yAxis": {
          "unit": "s"
        },
        "tooltip": {
          "show": true
        },
        "legend": {
          "show": true
        }
      }
    }
  ]
}
