    {
      "id": 11,
      "title": "OTLP Ingest Rate (Alloy)",
      "description": "Telemetry accepted by Alloy OTLP receivers",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 9,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "MIMIR"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(otelcol_receiver_accepted_log_records{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Logs/s"
        },
        {
          "refId": "B",
          "expr": "sum(rate(otelcol_receiver_accepted_metric_points{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Metrics/s"
        },
        {
          "refId": "C",
          "expr": "sum(rate(otelcol_receiver_accepted_spans{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Spans/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "mean",
            "max"
          ]
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
