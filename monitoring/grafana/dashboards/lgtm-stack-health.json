    {
      "id": 16,
      "title": "Queue Health",
      "description": "Exporter queue utilization (backpressure indicator)",
      "type": "gauge",
      "gridPos": {
        "x": 0,
        "y": 26,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "MIMIR"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(otelcol_exporter_queue_size{source=\"pipeline\"} / otelcol_exporter_queue_capacity{source=\"pipeline\"} * 100) or vector(0)",
          "legendFormat": "Queue %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
