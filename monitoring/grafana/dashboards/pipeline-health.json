{
  "uid": "scanium-pipeline-health",
  "title": "Scanium - Pipeline Health",
  "description": "Observability pipeline health: Alloy, Loki, Mimir, Tempo ingestion",
  "tags": ["scanium", "pipeline", "infrastructure"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "platform",
        "label": "Platform",
        "type": "custom",
        "query": "android,ios",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "options": [
          { "text": "All", "value": "$__all", "selected": true },
          { "text": "android", "value": "android", "selected": false },
          { "text": "ios", "value": "ios", "selected": false }
        ],
        "includeAll": true,
        "multi": true
      },
      {
        "name": "env",
        "label": "Environment",
        "type": "custom",
        "query": "dev,staging,prod",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "options": [
          { "text": "All", "value": "$__all", "selected": true },
          { "text": "dev", "value": "dev", "selected": false },
          { "text": "staging", "value": "staging", "selected": false },
          { "text": "prod", "value": "prod", "selected": false }
        ],
        "includeAll": true,
        "multi": true
      },
      {
        "name": "app_version",
        "label": "App Version",
        "type": "query",
        "datasource": { "type": "loki", "uid": "LOKI" },
        "query": "label_values({source=\"scanium-mobile\"}, app_version)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Log Ingestion Rate (Loki)",
      "description": "Logs ingested per minute",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"} [1m]))",
          "legendFormat": "Logs/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "green" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 30,
            "lineWidth": 2
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 2,
      "title": "Log Volume by Event Type",
      "description": "Log distribution by event prefix",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"} | json | event_name=~\"scan\\\\..*\" [5m]))",
          "legendFormat": "scan.*"
        },
        {
          "refId": "B",
          "expr": "sum(count_over_time({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"} | json | event_name=~\"ml\\\\..*\" [5m]))",
          "legendFormat": "ml.*"
        },
        {
          "refId": "C",
          "expr": "sum(count_over_time({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"} | json | event_name=~\"error\\\\..*\" [5m]))",
          "legendFormat": "error.*"
        },
        {
          "refId": "D",
          "expr": "sum(count_over_time({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"} | json | event_name=~\"ui\\\\..*\" [5m]))",
          "legendFormat": "ui.*"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "stacking": { "mode": "normal" }
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 3,
      "title": "Total Logs (1h)",
      "description": "Total log entries in last hour",
      "type": "stat",
      "gridPos": { "x": 0, "y": 8, "w": 6, "h": 4 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"} [1h]))",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "green", "value": 1000 }
            ]
          },
          "unit": "short",
          "noValue": "0"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background"
      }
    },
    {
      "id": 4,
      "title": "Avg Logs/min (1h)",
      "description": "Average log rate over last hour",
      "type": "stat",
      "gridPos": { "x": 6, "y": 8, "w": 6, "h": 4 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"} [1h])) / 60",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "short",
          "decimals": 1,
          "noValue": "0"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background"
      }
    },
    {
      "id": 5,
      "title": "Unique Sessions (1h)",
      "description": "Unique session_ids sending telemetry",
      "type": "stat",
      "gridPos": { "x": 12, "y": 8, "w": 6, "h": 4 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "targets": [
        {
          "refId": "A",
          "expr": "count(sum by (session_id) (count_over_time({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"} | json [1h])))",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "purple", "value": null }
            ]
          },
          "unit": "short",
          "noValue": "0"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background"
      }
    },
    {
      "id": 6,
      "title": "Data Freshness",
      "description": "Time since last log received",
      "type": "stat",
      "gridPos": { "x": 18, "y": 8, "w": 6, "h": 4 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "targets": [
        {
          "refId": "A",
          "expr": "time() - max(timestamp({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"}))",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 300 },
              { "color": "red", "value": 900 }
            ]
          },
          "unit": "s",
          "noValue": "No data"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background"
      }
    },
    {
      "id": 7,
      "title": "Event Distribution",
      "description": "Distribution of event types",
      "type": "piechart",
      "gridPos": { "x": 0, "y": 12, "w": 8, "h": 8 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (event_name) (count_over_time({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"} | json [1h]))",
          "instant": true,
          "legendFormat": "{{event_name}}"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "pieType": "donut"
      }
    },
    {
      "id": 8,
      "title": "Metrics Series Count (Mimir)",
      "description": "Active metric series in Mimir",
      "type": "timeseries",
      "gridPos": { "x": 8, "y": 12, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "count({source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"})",
          "legendFormat": "Active series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "orange" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 30,
            "lineWidth": 2
          },
          "unit": "short",
          "noValue": "No metrics"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 9,
      "title": "Trace Count (Tempo)",
      "description": "Traces via TraceQL service search",
      "type": "stat",
      "gridPos": { "x": 16, "y": 12, "w": 8, "h": 4 },
      "datasource": { "type": "tempo", "uid": "TEMPO" },
      "targets": [
        {
          "refId": "A",
          "queryType": "traceql",
          "query": "{resource.service.name =~ \"scanium-mobile\"}",
          "limit": 100
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "short",
          "noValue": "N/A"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["count"] },
        "colorMode": "background"
      }
    },
    {
      "id": 10,
      "title": "Pipeline Status",
      "description": "Quick health check for observability pipeline",
      "type": "table",
      "gridPos": { "x": 16, "y": 16, "w": 8, "h": 4 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "targets": [
        {
          "refId": "A",
          "expr": "vector(1)",
          "instant": true,
          "legendFormat": "Loki"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "1": { "text": "OK", "color": "green" } } }
          ]
        }
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "reduce",
          "options": { "reducers": ["last"] }
        }
      ]
    },
    {
      "id": 11,
      "title": "Recent Log Stream",
      "description": "Live log stream from mobile app",
      "type": "logs",
      "gridPos": { "x": 0, "y": 20, "w": 24, "h": 8 },
      "datasource": { "type": "loki", "uid": "LOKI" },
      "targets": [
        {
          "refId": "A",
          "expr": "{source=\"scanium-mobile\", platform=~\"$platform\", env=~\"$env\", app_version=~\"$app_version\"}"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending"
      }
    },
    {
      "id": 100,
      "title": "Pipeline Self-Observability",
      "type": "row",
      "gridPos": { "x": 0, "y": 28, "w": 24, "h": 1 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 101,
      "title": "Service Status",
      "description": "Health status of observability services (1 = up, 0 = down)",
      "type": "stat",
      "gridPos": { "x": 0, "y": 29, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "up{source=\"pipeline\"}",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" } } },
            { "type": "value", "options": { "1": { "text": "UP", "color": "green" } } }
          ],
          "unit": "short"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "" },
        "colorMode": "background",
        "graphMode": "none",
        "orientation": "horizontal"
      }
    },
    {
      "id": 102,
      "title": "Alloy Build Info",
      "description": "Alloy version and build information",
      "type": "stat",
      "gridPos": { "x": 6, "y": 29, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "alloy_build_info{source=\"pipeline\"}",
          "legendFormat": "v{{version}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "" },
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "name"
      }
    },
    {
      "id": 103,
      "title": "Alloy Uptime",
      "description": "Time since Alloy process started",
      "type": "stat",
      "gridPos": { "x": 12, "y": 29, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "time() - process_start_time_seconds{job=\"alloy\", source=\"pipeline\"}",
          "legendFormat": "uptime"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "unit": "s"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background"
      }
    },
    {
      "id": 104,
      "title": "Scrape Health",
      "description": "Last successful scrape timestamp (seconds ago)",
      "type": "stat",
      "gridPos": { "x": 18, "y": 29, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "time() - scrape_samples_scraped{source=\"pipeline\"}",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 120 }
            ]
          },
          "unit": "s",
          "max": 300
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "orientation": "horizontal"
      }
    },
    {
      "id": 105,
      "title": "OTLP Receiver - Accepted Records",
      "description": "Records accepted by Alloy OTLP receivers (logs, metrics, spans)",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 33, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(otelcol_receiver_accepted_log_records{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Logs accepted/s"
        },
        {
          "refId": "B",
          "expr": "sum(rate(otelcol_receiver_accepted_metric_points{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Metrics accepted/s"
        },
        {
          "refId": "C",
          "expr": "sum(rate(otelcol_receiver_accepted_spans{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Spans accepted/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 106,
      "title": "OTLP Receiver - Refused Records",
      "description": "Records refused by Alloy OTLP receivers (indicates errors)",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 33, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(otelcol_receiver_refused_log_records{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Logs refused/s"
        },
        {
          "refId": "B",
          "expr": "sum(rate(otelcol_receiver_refused_metric_points{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Metrics refused/s"
        },
        {
          "refId": "C",
          "expr": "sum(rate(otelcol_receiver_refused_spans{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Spans refused/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": ".*" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 107,
      "title": "Exporter - Sent Records",
      "description": "Records successfully sent to backends (Loki, Mimir, Tempo)",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 41, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(otelcol_exporter_sent_log_records{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Logs sent/s"
        },
        {
          "refId": "B",
          "expr": "sum(rate(otelcol_exporter_sent_metric_points{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Metrics sent/s"
        },
        {
          "refId": "C",
          "expr": "sum(rate(otelcol_exporter_sent_spans{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Spans sent/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 108,
      "title": "Exporter - Send Failures",
      "description": "Records that failed to send (indicates backend issues)",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 41, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(otelcol_exporter_send_failed_log_records{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Logs failed/s"
        },
        {
          "refId": "B",
          "expr": "sum(rate(otelcol_exporter_send_failed_metric_points{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Metrics failed/s"
        },
        {
          "refId": "C",
          "expr": "sum(rate(otelcol_exporter_send_failed_spans{source=\"pipeline\"}[5m])) or vector(0)",
          "legendFormat": "Spans failed/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 30,
            "lineWidth": 2
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 109,
      "title": "Queue Size",
      "description": "Items currently queued for export (backpressure indicator)",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 49, "w": 8, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "MIMIR" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(otelcol_exporter_queue_size{source=\"pipeline\"}) by (exporter) or vector(0)",
          "legendFormat": "{{exporter}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 110,
      "title": "Queue Capacity",
